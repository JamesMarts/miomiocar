import 'package:flutter_riverpod/flutter_riverpod.dart';
import 'package:flutter_demo/app/core/di/injector.dart';

/// XXçŠ¶æ€
class XXState {
  /// æ•°æ®
  final List<XX> items;
  
  /// æ˜¯å¦æ­£åœ¨åŠ è½½
  final bool isLoading;
  
  /// é”™è¯¯ä¿¡æ¯
  final String? error;

  const XXState({
    this.items = const [],
    this.isLoading = false,
    this.error,
  });

  /// å¤åˆ¶å¹¶ä¿®æ”¹
  XXState copyWith({
    List<XX>? items,
    bool? isLoading,
    String? error,
  }) {
    return XXState(
      items: items ?? this.items,
      isLoading: isLoading ?? this.isLoading,
      error: error ?? this.error,
    );
  }
}

/// XXçŠ¶æ€ç®¡ç†
class XXNotifier extends StateNotifier<XXState> {
  final XXRepository _repository;

  XXNotifier(this._repository) : super(const XXState());

  /// åŠ è½½æ•°æ®
  Future<void> loadData() async {
    state = state.copyWith(isLoading: true, error: null);

    try {
      final items = await _repository.getItems();
      state = state.copyWith(
        items: items,
        isLoading: false,
      );
    } catch (e) {
      state = state.copyWith(
        isLoading: false,
        error: e.toString(),
      );
    }
  }
}

/// XX Provider
final xxProvider = StateNotifierProvider<XXNotifier, XXState>((ref) {
  final repository = getIt<XXRepository>();
  return XXNotifier(repository);
});
